{
    "name": "molo/composer-proxy",
    "description": "A Composer plugin for proxy management",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "James Williams",
            "email": "jameswilliamsmdl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer-plugin-api": "^2.3",
        "composer/composer": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Molo\\ComposerProxy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Molo\\ComposerProxy\\Test\\": "tests/"
        }
    },
    "extra": {
        "class": "Molo\\ComposerProxy\\Plugin",
        "plugin-modifies-downloads": true,
        "plugin-modifies-install-path": false,
        "plugin-modifies-repositories": true
    },
    "config": {
        "allow-plugins": {
            "molo/composer-proxy": true
        },
        "sort-packages": true
    }
}
